@model ASM.BD.Models.Seguridad

@if (Request.IsAuthenticated)
{
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container">
                <div class="nav-collapse">
                    <ul id="home-menu" class="nav">
                        <li  class="active"><a href="/"><span class="icon-home"></span></a></li>
                    </ul>
                        <ul id="menu-menu" class="nav">
                            @foreach (ASM.BD.Models.MenuASM m  in Model.MiMenu())
                            {
                                <li class="menu-item menu-item-type-post_type menu-item-object-page">@Html.ActionLink(m.Nombre,m.ActionName, m.ControllerName)</li>
                            }
                        </ul> 

                            
                </div> <!--/.nav-collapse -->
            </div>
        </div>
    </div>
}
else
{ 
    <br /><br /><br /><br />
}
